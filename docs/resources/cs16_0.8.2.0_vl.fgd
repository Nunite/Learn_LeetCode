	fadein(integer) : "音量淡入时间长短 (0-100)" : 0
	fadeout(integer) : "音量淡出时间长短 (0-100)" : 0
//*main pitch frequency
	pitch(integer) : "音高 (100 = 正常)" : 100
//*begin pitch 
	pitchstart(integer) : "初始音高" : 0
//*how fast to change from begin pitch to main pitch.
	spinup(integer) : "音高淡入时间长短 (0-100)" : 0
//*how fast to go back down from main pitch to nothing. //* 不是"nothing"而是"begin pitch"
	spindown(integer) : "音高淡出时间长短 (0-100)" : 0
//*LFO = low frequency oscillator.
//*type of sound wave, sqr will jump back and forth between low and high pitch, tri has sharp transitions, and rnd has smooth transitions. //*rnd=random
	lfotype(choices) : "低频震荡类型" : 0 = 
	[
		0: "无"
		1: "跳跃"
		2: "波动"
		3: "随机改变"
	]
//*rate is the frequency (how often the LFO effect repeats itself).
	lforate(integer) : "低频震荡频率高低 (0-1000)" : 0
//*how much pitch change,  This effect is commonly called "vibrato" in music and song. 
	lfomodpitch(integer) : "低频震荡音高变化 (0-100)" : 0
//*how strong a "pulse" of volume rather than a wavering pitch.
	lfomodvol(integer) : "低频震荡音量变化 (0-100)" : 0
